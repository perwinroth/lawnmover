generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model ListingProposal {
  id          String   @id @default(cuid())
  createdAt   DateTime @default(now())
  title       String
  website     String
  priceFrom   Int?
  categories  String?
  description String?
}

