generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("PRISMA_DATABASE_URL")
}

model Seller {
  id          String  @id
  name        String
  website     String?
  address     String?
  city        String?
  postcode    String?
  street      String?
  housenumber String?
  lat         Float?
  lon         Float?
}

